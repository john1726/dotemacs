REM Main Release build 
rem echo off
rem set arg1=%1
rem xcopy /E /I /Y C:\TeamCity\buildAgent\work\fbef456932ac7c99\OSI\QI-Pro\x64\Release\QI-Pro.exe .
rem xcopy /E /I /Y C:\TeamCity\buildAgent\work\fbef456932ac7c99\IMAC\ReleaseNotes.txt .
rem makensis.exe QiPro.nsi
rem mkdir Z:\Applications\QI-Pro\PreRelease\%arg1%
rem xcopy /E /I /Y config.dat Z:\Applications\QI-Pro\PreRelease\%arg1%
rem xcopy /E /I /Y QI-Pro.exe Z:\Applications\QI-Pro\PreRelease\%arg1%
rem xcopy /E /I /Y ReleaseNotes.txt Z:\Applications\QI-Pro\PreRelease\%arg1%
rem xcopy /E /I /Y QIProSetup.exe Z:\Applications\QI-Pro\PreRelease\%arg1%

REM Main Release build with SeisWare 10.3
xcopy /E /I /Y C:\TeamCity\buildAgent\work\fbef456932ac7c99\OSI\QI-Pro\x64\SW-v10.3\QI-Pro.exe Seisware10.3
xcopy /E /I /Y C:\TeamCity\buildAgent\work\fbef456932ac7c99\IMAC\ReleaseNotes.txt Seisware10.3
makensis.exe Seisware10.3\QiProSeisware.nsi        
mkdir Z:\Applications\QI-Pro\PreRelease\%arg1%\Seisware10.3
copy Seisware10.3\config.dat Z:\Applications\QI-Pro\PreRelease\%arg1%\Seisware10.3\config.dat
copy Seisware10.3\libzmq-mt-4_3_0.dll Z:\Applications\QI-Pro\PreRelease\%arg1%\Seisware10.3\libzmq-mt-4_3_0.dll
copy Seisware10.3\QI-Pro.exe Z:\Applications\QI-Pro\PreRelease\%arg1%\Seisware10.3\QI-Pro.exe
copy Seisware10.3\ReleaseNotes.txt Z:\Applications\QI-Pro\PreRelease\%arg1%\Seisware10.3\ReleaseNotes.txt
copy Seisware10.3\QIProSetup.exe Z:\Applications\QI-Pro\PreRelease\%arg1%\Seisware10.3\QIProSetup.exe
copy Seisware10.3\seisware.dll Z:\Applications\QI-Pro\PreRelease\%arg1%\Seisware10.3\seisware.dll
copy Seisware10.3\vcruntime140_1.dll Z:\Applications\QI-Pro\PreRelease\%arg1%\Seisware10.3\vcruntime140_1.dll

